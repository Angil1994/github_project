body
  background #efeff4
  margin 0
ul,li
  padding 0
  list-style: none
  margin 0
.header
  font-size 20px
  text-align center
  background #FFFFFF
  color #ea5404
  font-family "Arial","Microsoft YaHei","黑体","宋体",sans-serif
  line-height 20px
  padding 3% 3%
  border-bottom 1px solid #d6d7dc
  img
    width 18px
  .icon_back
    float left
  .login
    font-size 14px
    color: #333
    float right
section
  margin-top 6%
  background #ffffff
  font-size 18px
  padding 0 12px
  border-top 1px solid #d6d7dc
  border-bottom 1px solid #d6d7dc
  color #333333
  .cell
    height 50px
    line-height 50px
    img
      width 20px
    input
      height 30px
      border 0
      outline none
      width 36%
    .get_code
      float right
      width 26%
      height 44%
      font-size 12px
      margin-top 4%
      background #ea5404
      border 0
      color #ffffff
      border-radius 4px
  #select_box
    position relative
    .select_icon
      position absolute
      top 40%
      right 0
      width 18px
      transform rotate(0deg)
  .options
    border-radius 4px
    position absolute
    right 0
    top 75%
    background #a3a3a3
    z-index 2
    height 0
    overflow hidden
    li
      padding 8px 11px
      border-bottom 1px solid #e1e1e1
      color white
      font-size 14px
      line-height normal
  .line
    height 1px
    border-top 1px Solid #d6d7dc
.btn_register
  width 90%
  position relative
  left 12px
  right 12px
  margin-top 40px
  height 44px
  font-size 18px
  color #ffffff
  background #c7c7c7
  border 0
  outline none
  border-radius 5px
.agreement
  width 90%
  color: #666666
  text-align center
  font-size 10px
  padding-top 12px
  a
    color: #ea5404
    text-decoration none
    padding-bottom 2px
    border-bottom 1px solid #ea5404



